    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "knex": "^0.19.0",
    "md5": "^2.2.1",
    "migrate": "^1.6.2",
    "module-alias": "^2.2.0",
    "morgan": "~1.9.1",
    "mysql": "^2.17.1",
    "pug": "2.0.0-beta11",
